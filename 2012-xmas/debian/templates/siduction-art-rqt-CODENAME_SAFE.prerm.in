#!/bin/sh
# postinst script for siduction-art-rqt-@CODENAME_SAFE@
#
# see: dh_installdeb(1)

set -e

case "$1" in
    remove|upgrade|deconfigure)
    rm -rf /etc/xdg/razor/razor.conf
    ;;

    failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
