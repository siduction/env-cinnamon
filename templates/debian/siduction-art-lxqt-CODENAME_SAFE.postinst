#!/bin/sh
# postinst script for siduction-art-rqt-@CODENAME_SAFE@
#
# see: dh_installdeb(1)

set -e

case "$1" in
    configure)
        if [ -f usr/share/lxqt/themes/@CODENAME_SAFE@/lxqt.conf ]; then
            mkdir -p /etc/xdg/lxqt
            cp -a usr/share/lxqt/themes/@CODENAME_SAFE@/lxqt.conf /etc/xdg/lxqt
        fi
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0

